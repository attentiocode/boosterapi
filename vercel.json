{
    "functions": {
      "api/**.ts": { "runtime": "nodejs20.x", "memory": 256, "maxDuration": 10 }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "https://ikg-businessbooster.webflow.io",
          "Access-Control-Allow-Methods": "GET,OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type,Authorization"
        },
        "continue": true
      },
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
  }
  